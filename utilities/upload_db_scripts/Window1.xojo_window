	#tag Method, Flags = &h21
		Private Function connect_database() As Boolean
		  db = New PostgreSQLDatabase
		  db.UserName = "transfer_monkey"
		  db.Password = "this monkey transfers data"
		  db.Host = "45.32.72.207"
		  db.Port = 5432
		  db.DatabaseName = "otis_data"
		  
		  If Not db.Connect Then
		    MsgBox( "Could not connect to database: " + db.ErrorMessage )
		    Return False
		  Else
		    Return True
		  End If
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub load_script(script_number as integer)
		  
		  
		  If script_number > -1 And script_number < order_.Ubound + 1 Then
		    
		    // Set existing fields
		    TextArea_script_contents.Text = script_contents( script_number )
		    TextField_order_.Text = order_( script_number ).ToText
		    TextField_script_name.Text = script_name( script_number )
		    TextField_switch_db.Text = switch_db( script_number )
		    
		    update_label_list
		    
		    label_current_script.Text = script_number.ToText
		    
		  Else
		    
		    MsgBox( "script number " + script_number.ToText + " does not exist" )
		    
		  End If
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub pull_from_db()
		  dim ps as PostgreSQLPreparedStatement
		  dim sql as string
		  dim rs as RecordSet
		  dim n1, n2, n3, n4 as integer
		  
		  
		  sql = "Select * From info_schema.db_creation Order By order_ ;"
		  ps = db.Prepare( sql )
		  rs = ps.SQLSelect
		  If db.Error Then
		    MsgBox( "Could not pull data: " + db.ErrorMessage )
		  End If
		  
		  
		  ReDim order_( -1 )
		  ReDim script_contents( -1 )
		  ReDim script_name( -1 )
		  ReDim switch_db( -1 )
		  
		  For i1 as integer = 1 To rs.RecordCount
		    
		    n1 = i1 - 1
		    ReDim order_( n1 )
		    ReDim script_contents( n1 )
		    ReDim script_name( n1 )
		    ReDim switch_db( n1 )
		    
		    order_(n1) = rs.Field( "order_" ).IntegerValue
		    script_contents(n1) = rs.Field( "script_contents" ).StringValue
		    script_name(n1) = rs.Field( "script_name" ).StringValue
		    switch_db(n1) = rs.Field( "switch_db" ).StringValue
		    
		    rs.MoveNext
		    
		  Next
		  
		  update_label_list
		  load_script( 0 )
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub update_label_list()
		  dim s1 as string
		  
		  
		  For i1 as Integer = 0 To order_.Ubound
		    
		    s1 = s1 + i1.ToText
		    
		    if i1 <> order_.Ubound Then
		      s1 = s1 + " "
		    end if
		    
		  Next
		  
		  label_list.Text = s1
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub update_script(i1 as integer)
		  dim ps as PostgreSQLPreparedStatement
		  dim sql as string
		  
		  
		  
		  
		  
		  sql = "Update info_schema.db_creation Set switch_db = $1, script_contents = $2 Where script_name = $3 ; "
		  ps = db.Prepare( sql )
		  
		  ps.Bind( 0, switch_db( i1 ) )
		  ps.Bind( 1, script_contents( i1 ) )
		  ps.Bind( 2, script_name( i1 ) )
		  
		  
		  ps.SQLExecute
		  If db.Error Then
		    MsgBox( "Could not create record: " + db.ErrorMessage )
		  End If
		  
		  
		End Sub
	#tag EndMethod


