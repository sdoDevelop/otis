#tag Events PushButton_next
	#tag Event
		Sub Action()
		  dim n1 as integer
		  
		  n1 = val( label_current_script.Text )
		  
		  n1 = n1 + 1
		  
		  If n1 > order_.Ubound Then
		    n1 = 0
		  End If
		  
		  load_script( n1 )
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton_back
	#tag Event
		Sub Action()
		  dim n1 as integer
		  
		  n1 = val( label_current_script.Text )
		  
		  n1 = n1 - 1
		  
		  if n1 < 0 Then
		    n1 = order_.Ubound
		  end if
		  
		  load_script( n1 )
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton_new
	#tag Event
		Sub Action()
		  dim n1, n2, n3, n4 as integer
		  
		  
		  
		  // Prep variables for adding information to the new script
		  n1 = script_name.Ubound
		  n2 = n1 + 1
		  redim order_( n2 )
		  redim script_contents( n2 )
		  redim script_name( n2 )
		  redim switch_db( n2 )
		  
		  update_label_list
		  
		  load_script( order_.Ubound )
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton_go
	#tag Event
		Sub Action()
		  dim ps as PostgreSQLPreparedStatement
		  dim sql as string
		  
		  
		  sql = "Delete From info_schema.db_creation ; "
		  ps = db.Prepare( sql )
		  ps.SQLExecute
		  If db.Error Then
		    MsgBox( "Could not delete records: " + db.ErrorMessage )
		  End If
		  
		  For i1 as integer = 0 To order_.Ubound
		    
		    sql = "Insert Into info_schema.db_creation ( script_name, order_, switch_db, script_contents ) Values ( $1, $2, $3, $4 ) ; "
		    ps = db.Prepare( sql )
		    
		    ps.Bind( 0, script_name( i1 ) )
		    ps.Bind( 1, order_( i1 ) )
		    ps.Bind( 2, switch_db( i1 ) )
		    ps.Bind( 3, script_contents( i1 ) )
		    
		    ps.SQLExecute
		    If db.Error Then
		      MsgBox( "Could not create record: " + db.ErrorMessage )
		    End If
		    
		  Next
		End Sub
	#tag EndEvent
